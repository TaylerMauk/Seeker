    <SolidColorBrush x:Key="WindowBorderBrush" Color="#FF323235" />

    <SolidColorBrush x:Key="BackgroundDarkBrush" Color="#FF1E1E1E" />
    <SolidColorBrush x:Key="BackgroundLightBrush" Color="#FF323235" />

    <SolidColorBrush x:Key="TitlebarTextDarkBrush" Color="#FF1E1E1E" />
    <SolidColorBrush x:Key="TitlebarTextLightBrush" Color="#FFD1D1D1" />
